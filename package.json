{
  "name": "ambie-provisioning-front-end",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "forever start dev-server/index.js && webpack-dashboard -t 'Ambie Provisioning' -- webpack-dev-server --colors --no-info",
    "build": "rm -rf ./build && NODE_ENV=\"production\" webpack",
    "lint": "eslint ./src ./webpack.config.js -f table --ext .js --ext .jsx || true",
    "preview": "NODE_ENV=\"production\" webpack-dashboard -t 'Preview Mode - Ambie Provisioning' -- webpack-dev-server"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "css-loader": "0.14.5",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^19.0.2",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "prop-types": "^15.5.8",
    "redux-logger": "^2.7.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.2.1"
  },
  "dependencies": {
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "es6-promise": "^3.1.2",
    "express": "^4.15.3",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.4",
    "react": "^15.5.0",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-slider": "^2.0.0",
    "react-dom": "^15.5.0",
    "react-redux": "^4.4.0",
    "react-router": "^3.0.0",
    "react-tabs": "^0.8.3",
    "redux": "^3.3.1",
    "redux-form": "^6.6.3",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.0.1",
    "reselect": "^3.0.0"
  },
  "jest": {
    "testRegex": "(/tests/.*|\\.(test|spec))\\.js$",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "description": "Ambie SD card provisioning tool front end",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@code.zoetrope.io:ambie/provisioning-front.git"
  },
  "author": "Zoetrope",
  "template": {
    "author": "Stanko",
    "url": "git+ssh://git@github.com/Stanko/react-redux-webpack2-boilerplate.git",
    "homepage": "https://github.com/Stanko/react-redux-webpack2-boilerplate#readme"
  }
}
